{
  "name": "ui-storybook",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "publish-on-npm": "cd ./dist/ui && npm publish ",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && storybook dev -p 6006",
    "build-storybook": "npm run docs:json && storybook build -o docs-build -s projects/ui/src/lib/ui/assets"
  },
  "dependencies": {
    "@angular/animations": "^15.2.1",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.1",
    "@angular/compiler": "^15.2.1",
    "@angular/core": "^15.2.1",
    "@angular/forms": "^15.2.1",
    "@angular/platform-browser": "^15.2.1",
    "@angular/platform-browser-dynamic": "^15.2.1",
    "@angular/router": "^15.2.1",
    "@phosphor-icons/web": "^2.0.3",
    "@storybook/addons": "^7.0.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/theming": "^7.0.12",
    "css-loader": "^5.2.6",
    "rxjs": "~7.5.0",
    "sass": "^1.59.3",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular/cli": "~15.2.8",
    "@angular/compiler-cli": "^15.2.9",
    "@babel/core": "^7.20.12",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-actions": "^7.0.12",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/addon-mdx-gfm": "^7.0.12",
    "@storybook/angular": "^7.0.12",
    "@storybook/testing-library": "^0.1.0",
    "@types/jasmine": "~4.0.0",
    "babel-loader": "^8.3.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.12",
    "storybook-dark-mode": "^3.0.0",
    "typescript": "~4.8.4"
  }
